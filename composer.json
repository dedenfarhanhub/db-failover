{
  "name": "dedenfarhanhub/db-failover",
  "description": "Laravel DB Failover + Telegram Notification + Circuit Breaker + Dynamic DB Switching",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SultanDB\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0",
    "guzzlehttp/guzzle": "^7.0",
    "predis/predis": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SultanDB\\SultanDBServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}